         noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:Foldout text="Foldout" value="true" tabindex="3" focusable="false" usage-hints="DynamicTransform" name="Fold"
                style="flex-shrink: initial; opacity: 1; overflow: visible;">
