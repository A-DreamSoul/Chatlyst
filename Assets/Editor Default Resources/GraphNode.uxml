            <ui:TextField picking-mode="Ignore" label="Character Name" value="filler text" text="filler text"
                          name="CharacterName"
                          style="align-items: auto; flex-direction: row; justify-content: flex-start; width: auto; height: auto; opacity: 1;"/>
